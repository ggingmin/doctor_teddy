  <div class="modal fade" id="seemorereview" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                     <div class="modal-dialog" role="document">
                      <div class="modal-content">
                      <div class="modal-header">
                       <h5 class="modal-title" id="exampleModalLabel">리뷰 보기</h5>
                       <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                       <span aria-hidden="true">&times;</span>
                       </button>
                      </div>
                      <div class="modal-body">
                       <% @reviews.order("id desc").each do |r| %>
                            <div class= "carouselsec">
                              <div class="container text-center my-3">
    <div class="row mx-auto my-auto">
        <div id="reviewmodalCarousel" class="carousel slide" data-ride="carousel">
            <div class="carousel-inner" role="listbox">
                <div class="carousel-item active">
                    <img class="d-block col-4 img-fluid" src="http://placehold.it/200x200?text=1">
                </div>
                <div class="carousel-item">
                    <img class="d-block col-4 img-fluid" src="http://placehold.it/200x200?text=2">
                </div>
                <div class="carousel-item">
                    <img class="d-block col-4 img-fluid" src="http://placehold.it/200x200?text=3">
                </div>
                <div class="carousel-item">
                    <img class="d-block col-4 img-fluid" src="http://placehold.it/200x200?text=4">
                </div>
                <div class="carousel-item">
                    <img class="d-block col-4 img-fluid" src="http://placehold.it/200x200?text=5">
                </div>
                <div class="carousel-item">
                    <img class="d-block col-4 img-fluid" src="http://placehold.it/200x200?text=6">
                </div>
            </div>
            <a class="carousel-control-prev" href="#reviewmodalCarousel" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#reviewmodalCarousel" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>
</div>
                            </div> 
              <div class= "reviewtext">
               <div class="row">
                <div class=" col-11 offset-1">
                <div class= "reviewModalhead">
                                    <span><%= r.user.name %></span>
                <span class="reviewdate">   
          
           <span class="reviewdate"> <%= r.created_at.strftime("%v") %></span>
                 <span class="seestars">  
                  <span id="stars_<%= r.id %>"></span>
                  </span>
                </span>
                
              </div>
               </div>
               </div> 
              <div class= "reviewModalbody">
                    
               <div class="indexline">
                <div class= "row">
                 <div class= "col-2 offset-1 indexicon">
                  <div class= "icon">한줄평</div>
                 </div>
                 <div class= "col-9 indexicontext">
                  <div class= "briefing"><%= r.comment %></div>
                 </div>
                </div>
               </div>  
                
               <div class="indexline">  
                <div class= "row">
                 <div class= "col-2 offset-1 indexicon">
                  <div class= "icon">진료동물</div>
                 </div>
                 <div class= "col-9 indexicontext">
                  <div class= "reviewanimal"><%= r.reviewanimal %></div>
                 </div>
                </div>
               </div>  
                
               <div class="indexline">  
                 <div class= "row">
                 <div class= "col-2 offset-1 indexicon">
                  <div class= "icon">진료항목</div>
                 </div>
                 <div class= "col-9 indexicontext">
                  <div class= "treatment"><%= r.treatment %></div>
                 </div>
                </div>
              </div> 
               
               <div class="indexline">    
                <div class= "row">
                 <div class= "col-2 offset-1 indexicon">
                  <div class= "icon">자유리뷰</div>
                 </div>
                 <div class= "col-9 indexicontext">
                  <div class= "reviewmore"><%= r.reviewmore %></div>
                 </div>
                </div>
               </div>   
                
               
             </div>
                                  
                              </div>
                      <% end %>
                      </div>
                      <div class="modal-footer">
                      <button type="button" class="btn btn-secondary" data-dismiss="modal">닫기</button>
                     
                     </div>
                     </div>
                     </div>
                     </div>
                     
                     
                     
<script type="text/javascript">
// review carousel 용 
 $('#reviewmodalCarousel').carousel({
  interval: 10000
})
$('.carousel .carousel-item').each(function(){
    var next = $(this).next();
    if (!next.length) {
    next = $(this).siblings(':first');
    }
    next.children(':first-child').clone().appendTo($(this));
    
    if (next.next().length>0) {
    next.next().children(':first-child').clone().appendTo($(this));
    }
    else {
      $(this).siblings(':first').children(':first-child').clone().appendTo($(this));
    }
}); 
// review carousel용 끝
</script>